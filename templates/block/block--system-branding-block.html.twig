{#
/**
 * @file
 * Template for the branding block.
 */
#}
{% embed '@vartheme_bs5/organisms/block/block.twig' with {
  html_tag: 'div',
} %}

	{% block content %}
		{% include '@vartheme_bs5/organisms/navbar/navbar-brand.twig' with {
    text: site_name,
    image: site_logo,
    height: 'auto',
    path: path('<front>'),
    alt: elements.content.site_name['#markup'] ~ ' logo' ,
  } %}
	{% endblock %}

{% endembed %}
